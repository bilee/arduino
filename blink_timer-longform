// blink timer long form

const int r = 2;
const int g = 3;
const int y = 4;

void setup (){
  pinMode(r, OUTPUT);
  pinMode(g, OUTPUT);
  pinMode(y, OUTPUT);
}
                                  //900 sec in 15 minutes
                                  //each LED cycle is 2 sec- on off  |need 450 cycles
